{
	"name": "express-boilerplate",
	"version": "1.0.0",
	"description": "Express boilerplate",
	"main": "src/index.ts",
	"type": "module",
	"scripts": {
		"build:production": "tsx ./scripts/Build.ts production",
		"build:development": "tsx ./scripts/Build.ts development",
		"clean:build": "rimraf ./build",
		"clean:logs": "rimraf ./Logs",
		"clean": "concurrently \"npm run clean:build\" \"npm run clean:logs\"",
		"start": "cross-env NODE_ENV=development tsx watch ./src/index.ts",
		"lint:build": "tsc --project tsconfig.eslint.json",
		"lint:check": "eslint --cache",
		"lint:fix": "eslint --cache --fix",
		"prettier:fix": "prettier -w \"./**/*.{ts,json,md,prettierrc}\"",
		"prettier:check": "prettier -c \"./**/*.{ts,json,md,prettierrc}\"",
		"types:check": "tsc --noEmit",
		"check": "npm run lint:check && npm run prettier:check && npm run types:check",
		"fix": "npm run lint:fix && npm run prettier:fix && npm run types:check",
		"prepare": "npm run lint:build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kennarddh/express-boilerplate"
	},
	"author": "kennarddh",
	"license": "MIT",
	"dependencies": {
		"@prisma/client": "^5.14.0",
		"argon2": "^0.40.3",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"express-validator": "^7.1.0",
		"helmet": "^7.1.0",
		"jsonwebtoken": "^9.0.2",
		"range-parser": "^1.2.1",
		"rate-limiter-flexible": "^5.0.3",
		"tiny-typed-emitter": "^2.1.0",
		"winston": "^3.13.0",
		"winston-daily-rotate-file": "^5.0.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@eslint/js": "^9.6.0",
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/compression": "^1.7.5",
		"@types/cookie-parser": "^1.4.7",
		"@types/cors": "^2.8.17",
		"@types/eslint": "^8.56.10",
		"@types/eslint__js": "^8.42.3",
		"@types/eslint-plugin-security": "^3.0.0",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/node": "^20.12.13",
		"@types/range-parser": "^1.2.7",
		"@typescript-eslint/utils": "^7.11.0",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"eslint": "^9.3.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-security": "^3.0.0",
		"globals": "^15.3.0",
		"prettier": "^3.2.5",
		"prisma": "^5.14.0",
		"rimraf": "^5.0.7",
		"tsc-alias": "^1.8.10",
		"tsx": "^4.11.0",
		"typescript": "^5.4.5",
		"typescript-eslint": "^7.11.0"
	}
}
