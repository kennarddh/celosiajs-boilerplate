{
	"name": "express-boilerplate",
	"version": "1.0.0",
	"description": "Express boilerplate",
	"main": "src/App.js",
	"scripts": {
		"build": "babel ./src --out-dir ./build && npm run build:swagger:production",
		"build:swagger:production": "swagger-cli bundle ./src/Swagger/Swagger.json --outfile ./build/Swagger.json --type json",
		"build:swagger": "swagger-cli bundle ./src/Swagger/Swagger.json --outfile ./src/Swagger.json --type json",
		"clean": "rimraf ./build",
		"clean:logs": "rimraf ./Logs",
		"start": "cross-env NODE_ENV=production node ./build/index.js",
		"dev": "cross-env NODE_ENV=development nodemon --exec npx babel-node src/index.js",
		"lint:check": "eslint src/**/*.{js,json}",
		"lint:fix": "eslint --fix src/**/*.{js,json}",
		"prettier:fix": "prettier -w \"src/**/*.{js,json}\"",
		"prettier:check": "prettier -c \"src/**/*.{js,json}\"",
		"prepare": "husky install",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:watch:all": "jest --watchAll",
		"test:coverage": "jest --coverage",
		"test:changed": "jest -o",
		"pre-commit": "lint-staged -c \"./lintstagedrc.json\" --quiet"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kennarddh/express-boilerplate"
	},
	"keywords": [
		"nodejs",
		"boilerplate",
		"babel",
		"jwt",
		"express",
		"mongodb",
		"eslint",
		"bcrypt",
		"jsonwebtoken"
	],
	"author": "kennarddh",
	"license": "ISC",
	"dependencies": {
		"bcrypt": "^5.0.1",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.0.2",
		"express": "^4.17.2",
		"express-mongo-sanitize": "^2.2.0",
		"express-rate-limit": "^6.5.2",
		"express-validator": "^6.14.2",
		"helmet": "^6.0.0",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^6.6.2",
		"swagger-ui-express": "^4.5.0",
		"winston": "^3.8.2",
		"winston-daily-rotate-file": "^4.7.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.18.10",
		"@babel/core": "^7.19.3",
		"@babel/node": "^7.19.1",
		"@babel/plugin-transform-runtime": "^7.19.1",
		"@babel/preset-env": "^7.19.3",
		"@babel/runtime": "^7.19.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.24.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jest": "^27.0.2",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-security": "^1.5.0",
		"husky": "^8.0.1",
		"jest": "^29.0.3",
		"lint-staged": "^13.0.3",
		"mockingoose": "^2.15.2",
		"nodemon": "^2.0.20",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"supertest": "^6.2.4",
		"swagger-cli": "^4.0.4"
	}
}
