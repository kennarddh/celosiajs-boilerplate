{
	"name": "express-boilerplate",
	"version": "1.0.0",
	"description": "Express boilerplate",
	"main": "src/index.ts",
	"scripts": {
		"build:production": "ts-node ./scripts/Build.ts production",
		"build:development": "ts-node ./scripts/Build.ts development",
		"build:swagger": "swagger-cli bundle ./src/Swagger/Swagger.json --outfile ./src/Swagger.json --type json",
		"watch:swagger": "ts-node ./scripts/Swagger/Watch/index.ts",
		"preview:swagger": "concurrently \"npm run watch:swagger\" \"ts-node ./scripts/Swagger/Preview/index.ts\"",
		"clean:build": "rimraf ./build",
		"clean:logs": "rimraf ./Logs",
		"clean:dist": "rimraf ./dist",
		"clean:coverage": "rimraf ./coverage",
		"clean": "concurrently \"npm run clean:build\" \"npm run clean:logs\" \"npm run clean:dist\" \"npm run clean:coverage\"",
		"start": "cross-env NODE_ENV=development concurrently \"npm run build:swagger\" \"npm run watch:swagger\" \"npx tsc --watch\" \"npx tsc-alias -w -p tsconfig.json\" \"nodemon -q dist/index.js\"",
		"lint:check": "eslint src/**/*.{ts,json} scripts/**/*.ts && tsc --noemit",
		"lint:fix": "eslint --fix src/**/*.{ts,json} scripts/**/*.ts && tsc --noemit",
		"prettier:fix": "prettier -w \"src/**/*.{ts,json}\" \"scripts/**/*.{ts,html}\"",
		"prettier:check": "prettier -c \"src/**/*.{ts,json}\" \"scripts/**/*.{ts,html}\"",
		"prepare": "husky && npm run build:swagger",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:watch:all": "jest --watchAll",
		"test:coverage": "jest --coverage",
		"test:changed": "jest -o",
		"lint-staged": "lint-staged"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kennarddh/express-boilerplate"
	},
	"author": "kennarddh",
	"license": "MIT",
	"dependencies": {
		"@prisma/client": "^5.9.1",
		"bcrypt": "^5.1.1",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.4.2",
		"express": "^4.18.2",
		"express-mongo-sanitize": "^2.2.0",
		"express-rate-limit": "^7.1.5",
		"express-validator": "^7.0.1",
		"helmet": "^7.1.0",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^8.1.2",
		"swagger-ui-express": "^5.0.0",
		"winston": "^3.11.0",
		"winston-daily-rotate-file": "^5.0.0"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/bcrypt": "^5.0.2",
		"@types/compression": "^1.7.5",
		"@types/cookie-parser": "^1.4.6",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.12",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/node": "^20.11.17",
		"@types/supertest": "^6.0.2",
		"@types/swagger-ui-dist": "^3.30.4",
		"@types/swagger-ui-express": "^4.1.6",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"eslint": "^8.56.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jest": "^27.6.3",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-security": "^2.1.0",
		"husky": "^9.0.10",
		"jest": "^29.7.0",
		"lint-staged": "^15.2.2",
		"mockingoose": "^2.16.2",
		"nodemon": "^3.0.3",
		"prettier": "^3.2.5",
		"prisma": "^5.9.1",
		"rimraf": "^5.0.5",
		"supertest": "^6.3.4",
		"swagger-cli": "^4.0.4",
		"swagger-ui-dist": "^5.11.3",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"tsc-alias": "^1.8.8",
		"typescript": "^5.3.3"
	}
}
