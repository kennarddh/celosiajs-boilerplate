{
	"name": "express-boilerplate",
	"version": "1.0.0",
	"description": "Express boilerplate",
	"main": "src/index.ts",
	"type": "module",
	"scripts": {
		"build:production": "tsx ./scripts/Build.ts production",
		"build:development": "tsx ./scripts/Build.ts development",
		"clean:build": "rimraf ./build",
		"clean:logs": "rimraf ./Logs",
		"clean": "concurrently \"npm run clean:build\" \"npm run clean:logs\"",
		"start": "cross-env NODE_ENV=development tsx watch ./src/index.ts",
		"lint:build": "tsc --project tsconfig.eslint.json",
		"lint:check": "eslint && tsc --noemit",
		"lint:fix": "eslint --fix && tsc --noemit",
		"lint:staged": "lint-staged",
		"prettier:fix": "prettier -w \"./src/**/*.{ts,json}\" \"./scripts/**/*.{ts}\"",
		"prettier:check": "prettier -c \"./src/**/*.{ts,json}\" \"./scripts/**/*.{ts}\"",
		"prepare": "npm run lint:build && husky"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kennarddh/express-boilerplate"
	},
	"author": "kennarddh",
	"license": "MIT",
	"dependencies": {
		"@prisma/client": "^5.9.1",
		"argon2": "^0.31.2",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.4.2",
		"express": "^4.19.2",
		"express-rate-limit": "^7.1.5",
		"express-validator": "^7.0.1",
		"helmet": "^7.1.0",
		"jsonwebtoken": "^9.0.2",
		"range-parser": "^1.2.1",
		"tiny-typed-emitter": "^2.1.0",
		"winston": "^3.11.0",
		"winston-daily-rotate-file": "^5.0.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/compression": "^1.7.5",
		"@types/cookie-parser": "^1.4.6",
		"@types/cors": "^2.8.17",
		"@types/eslint": "^8.56.2",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/node": "^20.11.17",
		"@types/range-parser": "^1.2.7",
		"@typescript-eslint/utils": "^7.0.1",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"eslint": "^8.56.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-security": "^2.1.0",
		"globals": "^14.0.0",
		"husky": "^9.0.10",
		"lint-staged": "^15.2.2",
		"prettier": "^3.2.5",
		"prisma": "^5.9.1",
		"rimraf": "^5.0.5",
		"tsc-alias": "^1.8.8",
		"tsx": "^4.7.1",
		"typescript": "^5.3.3",
		"typescript-eslint": "^7.0.1"
	}
}
