{
	"name": "express-boilerplate",
	"version": "1.0.0",
	"description": "Express boilerplate",
	"main": "src/index.ts",
	"scripts": {
		"build:production": "ts-node ./scripts/Build.ts production",
		"build:development": "ts-node ./scripts/Build.ts development",
		"build:swagger": "swagger-cli bundle ./src/Swagger/Swagger.json --outfile ./src/Swagger.json --type json",
		"watch:swagger": "ts-node ./scripts/Swagger/Watch/index.ts",
		"preview:swagger": "concurrently \"npm run watch:swagger\" \"ts-node ./scripts/Swagger/Preview/index.ts\"",
		"clean": "rimraf ./build",
		"clean:logs": "rimraf ./Logs",
		"clean:dist": "rimraf ./dist",
		"clean:coverage": "rimraf ./coverage",
		"clean:all": "concurrently \"npm run clean\" \"npm run clean:logs\" \"npm run clean:dist\" \"npm run clean:coverage\"",
		"start": "cross-env NODE_ENV=development concurrently \"npm run build:swagger\" \"npm run watch:swagger\" \"npx tsc --watch\" \"npx tsc-alias -w -p tsconfig.json\" \"nodemon -q dist/index.js\"",
		"lint:check": "eslint src/**/*.{ts,json} scripts/**/*.ts && tsc --noemit",
		"lint:fix": "eslint --fix src/**/*.{ts,json} scripts/**/*.ts && tsc --noemit",
		"prettier:fix": "prettier -w \"src/**/*.{ts,json}\" \"scripts/**/*.{ts,html}\"",
		"prettier:check": "prettier -c \"src/**/*.{ts,json}\" \"scripts/**/*.{ts,html}\"",
		"prepare": "husky install && npm run build:swagger",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:watch:all": "jest --watchAll",
		"test:coverage": "jest --coverage",
		"test:changed": "jest -o",
		"pre-commit": "lint-staged -c \"./lintstagedrc.json\" --quiet"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kennarddh/express-boilerplate"
	},
	"keywords": [
		"nodejs",
		"boilerplate",
		"jwt",
		"express",
		"mongodb",
		"eslint",
		"bcrypt",
		"jsonwebtoken",
		"typescript"
	],
	"author": "kennarddh",
	"license": "MIT",
	"dependencies": {
		"bcrypt": "^5.1.1",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-mongo-sanitize": "^2.2.0",
		"express-rate-limit": "^6.9.0",
		"express-validator": "^7.0.1",
		"helmet": "^7.0.0",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^7.5.0",
		"swagger-ui-express": "^5.0.0",
		"winston": "^3.10.0",
		"winston-daily-rotate-file": "^4.7.1"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^4.2.0",
		"@types/bcrypt": "^5.0.0",
		"@types/compression": "^1.7.2",
		"@types/cookie-parser": "^1.4.3",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/jest": "^29.5.3",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/node": "^20.5.1",
		"@types/supertest": "^2.0.12",
		"@types/swagger-ui-dist": "^3.30.1",
		"@types/swagger-ui-express": "^4.1.3",
		"@typescript-eslint/eslint-plugin": "^6.5.0",
		"@typescript-eslint/parser": "^6.5.0",
		"concurrently": "^8.2.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.45.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jest": "^27.2.2",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-security": "^1.7.1",
		"husky": "^8.0.3",
		"jest": "^29.6.2",
		"lint-staged": "^14.0.0",
		"mockingoose": "^2.16.2",
		"nodemon": "^2.0.22",
		"prettier": "^3.0.2",
		"rimraf": "^5.0.1",
		"supertest": "^6.3.3",
		"swagger-cli": "^4.0.4",
		"swagger-ui-dist": "^5.4.2",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"tsc-alias": "^1.8.7",
		"typescript": "^5.1.6"
	}
}
